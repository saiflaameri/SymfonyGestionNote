              <div class="card">
                <div class="card-header card-header-primary">
                  <h4 class="card-title ">Note</h4>
                  <p class="card-category"> Voici une liste de vos Note</p>
                </div>

                <br>
                 <div class="card-header card-header-secondery">
                <div class="d-grid gap-2">
                  <button  class="btn btn-dark"><a href="enseignantajout/ajouterenote"> Ajouter Note
                
                </button></div></div>
                
                <div class="card-body">
                  <div class="table-responsive">
                    <table class="table">
                      <thead class=" text-primary">
                        
                        <tr>
                        <th> id</th>
                          <th> Nom</th>
                            <th> Prenom</th>
                              <th> Cin</th>
                                <th> Classe</th>
                        <th> Note tp</th>
                        <th>Note ds</th>
                        <th>Note exame</th>
                        
                        <th>Delete</th>
                        <th>update</th>
                        </tr>
                        <tr {% for  Note in note %}>
                            <td>{{ Note .id}}</td>
                            <td>{{ Note .nom}}</td>
                            <td>{{ Note .prenom}}</td>
                            <td>{{ Note .cin}}</td>
                            <td>{{ Note .classe}}</td>
                            <td>{{ Note .notetp}}</td>
                            <td>{{ Note .noteds}}</td>
                            <td>{{ Note .noteexame}}</td>
                           
                            <td><button type="button" class="btn btn-warning"> <a href="{{path('deletenote',{'id':Note.id})}}">Supprimer</button></td>
                            <td> <button type="button" class="btn btn-success"><a href="{{path('Update',{'id':Note.id})}}">Update</button></td>
                        </tr {% endfor %}>
                        

                        </table>  
                      </tbody>
                    </table>
